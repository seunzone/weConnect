    "validator": "^9.4.1",
    "volleyball": "^1.4.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.2",
    "webpackbar": "^2.6.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "babel-jest": "^23.4.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.6",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^23.4.1",
    "moxios": "^0.4.0",
    "nodemon": "^1.17.1",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.3"
  },
  "jest": {
    "collectCoverage": true,
    "verbose": true,
    "testEnvironment": "node",
    "setupFiles": [
      "./client/__test__/setup.config.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/client/__tests__/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/client/__tests__/__mocks__/styleMock.js"
    },
    "testMatch": [
      "**/?(*.)(test).js?(x)",
      "**/__tests__/?(*.)(spec|test).js?(x)"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/client/src/__tests__"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>./server/test/*.js"
    ]
  }
}
